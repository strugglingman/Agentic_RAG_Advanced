# ==============================================================================
# Docker Compose Environment Variables
# ==============================================================================
# This file contains secrets for Docker deployment.
# Copy to .env.docker and fill in real values for production.
# NEVER commit real secrets to git!

# ==============================================================================
# PostgreSQL Database
# ==============================================================================
POSTGRES_USER=db_user
POSTGRES_PASSWORD=db_password
POSTGRES_DB=db_name

# ==============================================================================
# Authentication Secrets
# ==============================================================================
# Generate with: openssl rand -base64 32
NEXTAUTH_SECRET=this is a test next auth secret please change
SERVICE_AUTH_SECRET=this is a test service auth secret please change
SERVICE_AUTH_ISSUER=service_name
SERVICE_AUTH_AUDIENCE=service_audience
